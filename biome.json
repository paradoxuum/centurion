{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"files": {
		"ignore": ["./docs/.docusaurus", "./docs/build", "./out", "./test/out"]
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": "off"
			}
		}
	}
}
