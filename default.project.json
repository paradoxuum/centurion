{
	"name": "centurion-test",
	"globIgnorePaths": ["**/default.project.json"],
	"emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"pkgs": {
				"$className": "Folder",

				"DevPackages": {
					"$path": "DevPackages"
				},

				"centurion": {
					"$className": "Folder",
					"src": {
						"$path": "pkgs/centurion/src"
					}
				},

				"centurion-ui": {
					"$className": "Folder",

					"src": {
						"$path": "pkgs/centurion-ui/src"
					},

					"fluid": {
						"$path": "pkgs/centurion-ui/fluid.luau"
					},

					"centurion": {
						"$path": "pkgs/centurion-ui/centurion.luau"
					}
				}
			},
			"test": {
				"$path": "tests"
			}
		}
	}
}
